    "@remix-validated-form/with-zod": "^2.0.7",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "framer-motion": "^11.3.8",
    "isbot": "^4.1.0",
    "luxon": "^3.4.4",
    "morgan": "^1.10.0",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
